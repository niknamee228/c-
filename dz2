int main()
{
    int cheslo;
    int symma=0;
    cout<<"cheslo ";
    cin>>cheslo;
    cout<<"symma " " ";
    for (;cheslo>0;cheslo/=10)
    {
        symma+=cheslo%10;
    }
    cout<<symma;

}
