
#include <iostream>
using namespace std;

class car {
public:
	string name;
	string wheels;
	string dviratel;
	string massa;
	string h;
	void print() const
	{
		cout << "Name:" << name << "Wheels:" << wheels << "Dviratel:" << dviratel << "Massa" << massa <<"h:"<<h<< endl;
	}
	
};
class wheels : car
{
public:
	string razmeri;
};

class dviratel : car
{
public:
	string sil;
};

class Massa :car {
public:
	string massa;
};
class h :car {
	string visota;
};
